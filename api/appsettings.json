{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "[tenant].onmicrosoft.com",
    "TenantId": "[tenant].onmicrosoft.com",
    "Authority": "https://login.microsoftonline.com/[tenant].onmicrosoft.com/v2.0",
